    <link href="{{ asset('bundles/flockmain/css/formalize.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}

{% block body %}
    <div id="wrapper">

        <div id="header">
            <div id="logo"><img src="{{ asset('bundles/flockmain/images/flock.png') }}"/></div>
        </div>

        {% block content %}{% endblock %}
    </div>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/flockmain/js/jquery-1.5.2.min.js') }}"></script>
    <script src="{{ asset('bundles/flockmain/js/jquery.formalize.js') }}"></script>
{% endblock %}
